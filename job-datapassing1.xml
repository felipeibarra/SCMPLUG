<joblist>
  <job>
    <context>
      <options preserveOrder='true'>
        <option name='Mypassword' secure='true' value='pass' valueExposed='true' />
      </options>
    </context>
    <defaultTab>output</defaultTab>
    <description />
    <executionEnabled>true</executionEnabled>
    <group>Log Filter</group>
    <id>0f370cb8-26bf-4369-9699-b9b42d8201a4</id>
    <loglevel>INFO</loglevel>
    <name>Example1-keyValueData</name>
    <nodeFilterEditable>false</nodeFilterEditable>
    <scheduleEnabled>true</scheduleEnabled>
    <sequence keepgoing='false' strategy='node-first'>
      <command>
        <description>Filtered Data - Key Value Data</description>
        <plugins>
          <LogFilter type='key-value-data'>
            <config>
              <logData>true</logData>
              <regex>^\s*(Warranty.+)\s*:\s*(.+)$</regex>
            </config>
          </LogFilter>
        </plugins>
        <script><![CDATA[echo "WarrantyCo: Dell "
echo "WarrantyExpire: '1970-01-01' "
echo "WarrantyStatus: None"]]></script>
        <scriptargs />
      </command>
      <command>
        <description>Filtered Data - Highlight Output</description>
        <plugins>
          <LogFilter type='highlight-output'>
            <config>
              <fgcolor>red</fgcolor>
              <mode>bold</mode>
              <regex>^\s*(Warranty.+)</regex>
            </config>
          </LogFilter>
        </plugins>
        <script><![CDATA[echo "WarrantyCo: Dell "
echo "WarrantyExpire: '1970-01-01' "
echo "Phil: '1970-01-01' "
echo "Phil: '1970-01-01' "
echo "WarrantyStatus: None"
echo "Phil: None "
]]></script>
        <scriptargs />
      </command>
      <command>
        <description>Filtered Data - Mask Password</description>
        <plugins>
          <LogFilter type='mask-passwords'>
            <config>
              <color>red</color>
              <replacement>[SECURE]</replacement>
            </config>
          </LogFilter>
        </plugins>
        <script><![CDATA[echo @option.Mypassword@]]></script>
        <scriptargs />
      </command>
      <command>
        <plugins>
          <LogFilter type='progress-badges'>
            <config>
              <doNothing>false</doNothing>
              <mute>false</mute>
              <regex>^\s*(Warranty.+)</regex>
              <text>Filtered Output....</text>
            </config>
          </LogFilter>
          <LogFilter type='quiet-output'>
            <config>
              <loglevel>verbose</loglevel>
              <matchLoglevel>normal</matchLoglevel>
              <quietMatch>false</quietMatch>
              <regex>^\s*(Warranty.+)</regex>
            </config>
          </LogFilter>
        </plugins>
        <script><![CDATA[echo "WarrantyCo: Dell "
echo "WarrantyExpire: '1970-01-01' "
echo "Phil: '1970-01-01' "
echo "Phil: '1970-01-01' "
echo "WarrantyStatus: None"
echo "Phil: None "
]]></script>
        <scriptargs />
      </command>
    </sequence>
    <uuid>0f370cb8-26bf-4369-9699-b9b42d8201a4</uuid>
  </job>
</joblist>